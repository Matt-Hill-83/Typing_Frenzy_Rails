<%= render "shared/errors" %>

<div class='user-session-box new-user-box dialog-box' id='new-user-box'>
  <p class="my-dialog-font" id="">Sign Up</p>

  <form action="<%= users_url %>" method="post">
    <input
      type="hidden"
      name="authenticity_token"
      value="<%= form_authenticity_token %>"
      >
    <%= render partial: "layouts/user_pass" %>
  </form>
</div>
